<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" fillWidth="true" spacing="15" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="test.Controller">
  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
  </padding>
  <Button disable="true" text="Image Compression (JPEG/PNG)">
    </Button>
  <TextField fx:id="imagePath" editable="false" onMouseClicked="#onImagePathClick" promptText="Choose Image">
    </TextField>
  <Button fx:id="compressButton" onMouseClicked="#onCompressClick" style="-fx-background-color: rgb(77,102,204);" text="PROCESS IMAGE" textFill="#ffffffe3">
    </Button>
</VBox>
